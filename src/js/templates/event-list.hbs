{{#each this}}
<li class="event-list-item">
       
               <div class="img-wrapper">
        <img class="event-img"
                width="120"
                height="151"
            src="{{this.img}}"
            alt="{{this.name}}"/>
           </div>
               
          <h3 class="event-title">{{this.name}}</h3>
          <p class="event-date">{{this.dates.start.localDate}}</p>
          {{#with this._embedded.venues}}
          <p class="event-location">{{[0].name}}</p>
          {{/with}}
      </li>
{{/each}}